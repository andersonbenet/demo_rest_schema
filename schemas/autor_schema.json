{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "ID",
      "IDBook",
      "FirstName",
      "LastName"
    ],
    "properties": {
      "ID": {
        "$id": "#/items/properties/ID",
        "type": "integer",
        "title": "The Id Schema",
        "default": 0,
        "examples": [
          1
        ]
      },
      "IDBook": {
        "$id": "#/items/properties/IDBook",
        "type": "integer",
        "title": "The Idbook Schema",
        "default": 0,
        "examples": [
          1
        ]
      },
      "FirstName": {
        "$id": "#/items/properties/FirstName",
        "type": "string",
        "title": "The Firstname Schema",
        "default": "",
        "examples": [
          "First Name 1"
        ],
        "pattern": "^(.*)$"
      },
      "LastName": {
        "$id": "#/items/properties/LastName",
        "type": "string",
        "title": "The Lastname Schema",
        "default": "",
        "examples": [
          "Last Name 1"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}